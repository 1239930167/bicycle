/**index.wxss**/
.container{
  height: 100vh; 
}
#myMap{
  width: 100%;
  height: 90%;
}
.container .main-btn{
  position: absolute;
  z-index: 10;
  background-color: #fff;
  text-align: center; 
  width:100%;
  bottom: 0;
}

.container .main-btn .banner{
  position: absolute;
  height: 95rpx;

  margin: 30rpx;
  left: 0%;
  bottom: 88vh;
}

.container .main-btn .main{
  display: block;
  background: #4A4C5B;
  margin: 20rpx;
  border-radius: 5rpx;
  height: 95rpx;
  color: white;
  line-height: 95rpx;
}