/**index.wxss**/
.container{
  height: 100vh;
}

#myMap{
  width: 100%;
  height: 90.5%;
}

.container .main-btn{
  position: absolute; 
  z-index: 9999999999999999;
  background-color: #fff;
  text-align: center;
  width:100%;
  bottom: 0;
  box-shadow: 0 0 10rpx #888888; 
}

.container .main-btn .main{
  display: block;
  background: #4A4C5B;
  margin: 20rpx;
  border-radius: 5rpx;
  height: 95rpx;
  color: white;
  line-height: 95rpx;
}